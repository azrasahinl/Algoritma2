
package algoritma2;

import java.io.File;
import java.io.FileNotFoundException;
import java.io.PrintWriter;

public class dosyayazmaislemiornek {
 //veli.txt dosyasına 1den300e kadar olan sayılar içinden
//13e tam bolunenleri yazalım  
    public static void main(String[] args) throws FileNotFoundException {
 File f=new File("veli.txt");
 PrintWriter pwr=new PrintWriter(f);
        for (int i =1; i <300; i++) {
  if(i%13==0) 
      pwr.println(i);
        }
 pwr.close();
}
}
