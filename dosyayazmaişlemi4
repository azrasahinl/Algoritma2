
package algoritma2;

import java.io.File;
import java.io.PrintWriter;

public class dosyayazmaislemi4 {
    public static void main(String[] args) {
        PrintWriter p=null;
        File dosya =new File("sonuc.txt");
        try{
            if(!dosya.exists())
                dosya.createNewFile();
            else
                System.out.println("Dosya mecvut yazilabilir");
           p=new PrintWriter(dosya);
           p.print("Ahmet TUNA\t");
           p.println(90);
           p.close();}
        catch(Exception e){
        }
           
        }
        
    }
  

