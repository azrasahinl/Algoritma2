
package algoritma2;
public class trycatchornek2 {
    public static void main(String[] args) {
     try{
         int a=30,b=0;
         int c=a/b;
         System.out.println("Sonuc"+c);
     }
     catch(ArithmeticException e){
         System.out.println("Sifira bolme hatasi");
     }     
     }
    }


